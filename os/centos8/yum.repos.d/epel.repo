baseurl=http://repo.home.local/artifactory/centos/$releasever/Everything/$basearch
#metalink=http://mirrors.fedoraproject.org/metalink?repo=epel-$releasever&arch=$basearch&infra=$infra&content=$contentdir
enabled=1
gpgcheck=1
gpgkey=http://repo.home.local/artifactory/centos/RPM-GPG-KEY-EPEL-8

